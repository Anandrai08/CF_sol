#include <iostream>
using namespace std;
int main() {
    long long n,m,a;
    cin>>n>>m>>a;
    long long val2, val1;

    if(n%a!=0){
        val1=n/a+1;
    }
    else{
        val1=n/a;
    }
    if(m%a!=0){
        val2=m/a+1;
    }
    else{
        val2=m/a;
    }

    cout<<val1*val2<<endl;

    return 0;
}
