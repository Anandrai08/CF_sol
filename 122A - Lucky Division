#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int arr[14]={4,7,44,77,47,74,477,447,444,777,774,744,747,474};
    int flag=0;
    for(int i=0;i<14;i++){
        if(n%arr[i]==0){
            flag++;
            break;
        }
    }
if(flag==1){
    cout<<"YES"<<endl;
}
else{
    cout<<"NO"<<endl;
}

    return 0;
}
